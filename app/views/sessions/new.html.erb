<h1>User Log In</h1>
<% if alert %>
  <%= alert %>
<% end %>

<%= form_with scope: :session, url: sessions_path, local: true do |form| %>


  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </p>
  <p>
    <%= form.label :password %><br>
    <%= form.password_field :password %>
  </p>

  <p>
    <%= form.submit %>
  </p>
  <% end %>

<%= link_to 'Homepage', welcome_index_path %>